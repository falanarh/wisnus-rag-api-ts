{
  "name": "wisnus-rag-api-ts",
  "version": "1.0.0",
  "description": "RAG API for Wisatawan Nusantara Survey using TypeScript, Express, Gemini AI, and MongoDB Atlas Vector Search",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && npm run copy-docs",
    "copy-docs": "node -e \"const fs=require('fs');const path=require('path');function copyDir(src,dest){if(!fs.existsSync(dest))fs.mkdirSync(dest,{recursive:true});const items=fs.readdirSync(src);items.forEach(item=>{const srcPath=path.join(src,item);const destPath=path.join(dest,item);if(fs.statSync(srcPath).isDirectory()){copyDir(srcPath,destPath);}else{fs.copyFileSync(srcPath,destPath);}});}copyDir('src/docs','dist/docs');console.log('âœ… Docs copied to dist/');\"",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "install:fast": "node install-fast.js",
    "test": "jest",
    "test:api": "node test-api.js",
    "test:concurrent": "node test-concurrent.js",
    "test:concurrent-endpoint": "node test-concurrent-endpoint.js",
    "test:deployment": "node test-deployment.js",
    "test:database": "node test-database-status.js",
    "test:source-cleaner": "node test-source-cleaner.js",
    "test:stress": "node test-stress.js",
    "test:monitor": "node test-monitor.js",
    "test:all": "node run-all-tests.js",
    "check:server": "node check-server.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "rag",
    "api",
    "typescript",
    "express",
    "gemini",
    "mongodb",
    "vector-search",
    "wisatawan-nusantara"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@langchain/core": "^0.3.61",
    "@langchain/google-genai": "^0.0.12",
    "@langchain/langgraph": "^0.3.5",
    "@langchain/mongodb": "^0.0.5",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "langsmith": "^0.3.34",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "@langchain/core": "^0.3.61"
  },
  "resolutions": {
    "@langchain/core": "^0.3.61"
  }
}
